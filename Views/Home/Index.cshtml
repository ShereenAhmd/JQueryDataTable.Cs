@{
    ViewData["Title"] = "Home Page";
}


<link href="~/datatables/css/datatables.bootstrap4.css" rel="stylesheet" />
<link href="~/datatables/css/datatables.bootstrap4.min.css" rel="stylesheet" />

<div class="container">
    <h3 class="text-primary">Customers</h3>
    <hr />
    <table id="Customers" class="table table-striped table-bordered dt-responsive nowrap">
        <thead>
            <tr>
                <th>Id</th>
                <th>First Name </th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Contact</th>
                <th>Date Of Birth</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>

</div>
@section Scripts 
    {

    <script src="~/datatables/js/datatables.bootstrap4.js"></script>
    <script src="~/datatables/js/datatables.bootstrap4.min.js"></script>


    <script src="~/datatables/js/jquery.datatables.js"></script>
    <script src="~/datatables/js/jquery.datatables.min.js"></script>
    <script src="~/js/customerdatatables.js"></script>
}